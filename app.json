{
  "expo": {
    "name": "tarik-react-native",
    "slug": "tarik-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "uses location",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "always use location",
        "NSLocationAlwaysUsageDescription": "using location"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
